{
    "extends": "tslint:recommended",
    "rules": {
      "indent": [
        true,
        "spaces",
        2
      ],
      "trailing-comma": [
        true,
        {
          "multiline": "never",
          "singleline": "never"
        }
      ],
      "no-empty": false,
      "no-var-requires": false,
      "interface-name": false
    }
  }
  